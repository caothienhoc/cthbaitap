<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @RenderSection("links", required: false)
    <link href="~/Content/Bootstrap/bootstrap.min.css" rel="stylesheet"/>
    <link href="~/Content/css/LayOut.css" rel="stylesheet"/>
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark ">
        <div class="container-fluid">
            <a class="navbar-brand" href="#">Bài tập</a>
            
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                  
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="@Url.Action("BaiMot","Home")">Bài 1</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="@Url.Action("BaiHai","Home")">Bài 2</a>

                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="@Url.Action("BaiBa","Home")">Bài 3</a>

                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="@Url.Action("BaiNam","JsonDemo")">Bài 5</a>

                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="@Url.Action("BaiSau","Home")">Bài 6</a>

                    </li>
                   
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="@Url.Action("BaiTam","Api")">Bài 8</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="@Url.Action("BaiChin","EasyUi")">Bài 9</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="@Url.Action("BaiMuoi","EasyUi")">Bài 10</a>
                    </li>

                </ul>

            </div>
        </div>
    </nav>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>
    <script src="~/Content/Bootstrap/jquery.slim.min.js"></script>
    <script src="~/Content/easyUi/jquery.min.js"></script>
    <script src="~/Content/Bootstrap/bootstrap.bundle.min.js"></script>
    <script defer src="https://use.fontawesome.com/releases/v5.15.4/js/all.js" integrity="sha384-rOA1PnstxnOBLzCLMcre8ybwbTmemjzdNlILg8O7z1lUkLXozs4DHonlDtnE7fpc" crossorigin="anonymous"></script>
    @RenderSection("scripts", required: false)

</body>
</html>
